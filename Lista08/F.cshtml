/*
 Desenvolva um programa que solicite ao usuário para inserir uma sequência
de números inteiros e, em seguida, exiba o maior número inserido.
 */
using System;

namespace F
{
	class Program
	{
		public static void Main(string[] args)
		{
			int n1,n2;
			string ternario;
			
			Console.WriteLine("Digite um numero");
			n1 = int.Parse(Console.ReadLine());
			
			Console.WriteLine("Digite um numero");
			n2 = int.Parse(Console.ReadLine());
			
			for( int i = 0; i < n1; i++ ){
				ternario = ( n1 < n2 ) ? "1° numero menor que o 2°" : "2° numero menor que o 1°";
				Console.WriteLine(ternario);
				break;
			}
			
			Console.Write("Press any key to continue . . . ");
			Console.ReadKey(true);
		}
	}
}